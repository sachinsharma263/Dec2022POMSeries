<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="trcrm app-Regression suite">
	<listeners>
		<!-- <listener class-name="com.qa.trcrm.listeners.pdfListener"></listener> -->
		<listener
			class-name="com.qa.trcrm.listeners.ExtentReportListener"></listener>
	</listeners>
	<test name="Login Page Test" verbose="2">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.qa.trcrm.tests.LoginPageTest2" />
		</classes>
	</test>
	<test name="Home Page Test" verbose="2">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="com.qa.trcrm.tests.HomePageTest2" />
		</classes>
	</test>



</suite> 
